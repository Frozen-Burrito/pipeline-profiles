{
  "name": "pipeline-profiles",
  "version": "1.2.1",
  "sideEffects": false,
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "node server.js",
    "heroku-postbuild": "webpack --config webpack.prod.js",
    "dev": "webpack serve --output-public-path=/dist/ --config webpack.dev.js",
    "update-traffic-data": "sh ./src/data_management/npm_scripts/update-traffic-data.sh",
    "update-incidents-data": "sh ./src/data_management/npm_scripts/update-incidents-data.sh",
    "update-conditions-data": "sh ./src/data_management/npm_scripts/update-conditions-data.sh",
    "update-apportionment-data": "sh ./src/data_management/npm_scripts/update-apportionment-data.sh",
    "update-oandm-data": "sh ./src/data_management/npm_scripts/update-oandm-data.sh",
    "maps": "sh ./src/data_management/npm_scripts/simplify-maps.sh",
    "test-backend": "sh ./src/data_management/npm_scripts/test-data.sh",
    "test-frontend": "ava",
    "save-conda-env": "sh ./src/data_management/npm_scripts/save-environment.sh",
    "set-remote-work": "git remote set-url origin https://CER-REC@dev.azure.com/CER-REC/Pipeline%20Information/_git/pipeline-profiles",
    "set-remote-personal": "git remote set-url origin https://github.com/mbradds/pipeline-profiles.git"
  },
  "author": "Grant Moss",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "haversine": "^1.1.1",
    "highcharts": "^9.1.2",
    "leaflet": "^1.7.1",
    "mapshaper": "^0.5.61"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "ava": "^3.15.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.15.2",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.1.0",
    "style-loader": "^3.1.0",
    "webpack": "^5.45.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "files": [
      "test/test.js"
    ],
    "extensions": {
      "js": true
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    }
